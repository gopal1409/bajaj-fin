pipeline {
    agent any 
    stages {
        stage('code from scm') {
           steps {
               git credentialsId: 'git-key', url: 'git@github.com:gopal1409/Jenkins-ci-baja.git'
           }
        }
        stage('mvn build') {
           steps {
               sh 'mvn -B -SskipTests clean package'
           }
        }
         stage('mvn test') {
           steps {
               echo "building code"
           }
        }
    }
}
