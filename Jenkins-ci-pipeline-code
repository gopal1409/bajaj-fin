pipeline {
    agent any 
    stages {
        stage('code from scm') {
           steps {
               echo "geeting code from scm"
           }
        }
        stage('mvn build') {
           steps {
               echo "building code"
           }
        }
         stage('mvn test') {
           steps {
               echo "building code"
           }
        }
    }
}
